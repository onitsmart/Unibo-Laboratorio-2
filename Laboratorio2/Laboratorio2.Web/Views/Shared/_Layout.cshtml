<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Laboratorio2.Web</title>
    <link href="~/css/bundle-global.css" asp-append-version="true" rel="stylesheet" />
    <link href="~/node_modules/@@fortawesome/fontawesome-free/css/all.css" asp-append-version="true" rel="stylesheet" />
    @RenderSection("styles", required: false)
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Laboratorio2.Web</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/News/2024/12/01/rischio-idrogeologico-in-aumento">Dettaglio news</a>
                        </li>
                        <li class="nav-item dropdown" style="cursor: pointer">
                            <a class="nav-link dropdown-toggle text-dark" data-bs-toggle="dropdown" aria-expanded="false">
                                Configura
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="/Configura/Utenti/Index">Utenti index</a></li>
                                <li><a class="dropdown-item" href="/Configura/Utenti/Edit/5813e1ce-c0c6-4195-9ccb-d3879060b0e7">Utenti edit</a></li>
                                <li><hr class="dropdown-divider" /></li>
                                <li><a class="dropdown-item" href="/Configura/Veicoli/Index">Veicoli index</a></li>
                                <li><a class="dropdown-item" href="/Configura/Veicoli/Edit/5813e1ce-c0c6-4195-9ccb-d3879060b0e7">Veicoli edit</a></li>
                                <li><a class="dropdown-item" href="/Configura/Veicoli/CheckPlate/OUTATIME">Veicoli check plate</a></li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown" style="cursor: pointer">
                            <a class="nav-link dropdown-toggle text-dark" data-bs-toggle="dropdown" aria-expanded="false">
                                Progetta
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="/Progetta/Grafici/Index">Grafici index</a></li>
                                <li><a class="dropdown-item" href="/Progetta/Grafici/Edit/5813e1ce-c0c6-4195-9ccb-d3879060b0e7">Grafici edit</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        @RenderBody()
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Laboratorio2.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/js/bundle-global.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
